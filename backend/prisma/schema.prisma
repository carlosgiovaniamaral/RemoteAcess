generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Computer {
  id           Int      @id @default(autoincrement())
  ip           String
  nameComputer String
  nameUser     String
  anydeskId    String
  setor        String
  ramal        String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
